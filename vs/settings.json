{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Operator Mono",
  "editor.fontSize": 14,
  "editor.fontWeight": "400",
  "editor.formatOnPaste": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "git.mergeEditor": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnPaste": false,
    "editor.tabSize": 4
  },
  "python.formatting.blackPath": "${workspaceFolder}/mamba/venv/bin/black",
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.pylintCategorySeverity.convention": "Hint",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.fontFamily": "Cartograph CF",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontWeight": "400",
  "terminal.integrated.fontWeightBold": "500",
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "tslint.jsEnable": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["Z", "Z"],
      "commands": [":wq"]
    }
  ],
  "vscode-pets.position": "explorer",
  "vscode-pets.theme": "forest",
  "workbench.colorCustomizations": {
    "editor.findMatchBackground": "#8077A8",
    "terminal.background": "#27144b",
    "terminal.foreground": "#E0E0E0",
    "terminalCursor.foreground": "#E0E0E0",
    "terminal.ansiBlack": "#44475A",
    "terminal.ansiBrightBlack": "#44475A",
    "terminal.ansiMagenta": "#E35557",
    "terminal.ansiBrightMagenta": "#E35557",
    "terminal.ansiGreen": "#A5CC95",
    "terminal.ansiBrightGreen": "#A5CC95",
    "terminal.ansiYellow": "#FFEA96",
    "terminal.ansiBrightYellow": "#FFEA96",
    "terminal.ansiBlue": "#d9c0ff",
    "terminal.ansiBrightBlue": "#d9c0ff",
    "terminal.ansiRed": "#ffb7d0",
    "terminal.ansiBrightRed": "#ffb7d0",
    "terminal.ansiCyan": "#b0e0fe",
    "terminal.ansiBrightCyan": "#b0e0fe",
    "terminal.ansiWhite": "#BFBFBF",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "workbench.colorTheme": "Noctis Lux",
  "workbench.panel.defaultLocation": "right",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "editor.cursorStyle": "line",
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "terminal.integrated.tabs.enabled": false,
  "workbench.iconTheme": "rose-pine-icons",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "git.autofetch": true,
  "python.testing.autoTestDiscoverOnSaveEnabled": false,
  "gitlens.codeLens.enabled": false,
  "explorer.experimental.fileNesting.enabled": true,
  "breadcrumbs.icons": false,
  "editor.formatOnSave": true,
  "githubPullRequests.pullBranch": "never"
}
